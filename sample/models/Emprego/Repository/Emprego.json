{
  "database": {
    "repository": "emprego",
    "keys": "ID"
  },
  "properties": [
    {
      "alias": "proID",
      "property": "ID",
      "description": "ID - campo incremental unico identificador do registro",
      "type": "int",
      "behavior": {
        "hidden": "true",
        "required": "false",
        "autoIncrement": "true"
      }
    },
    {
      "alias": "proCargo",
      "property": "cargo",
      "description": "cargo - nome do registro emprego",
      "type": "string",
      "format": [
        {
          "function": "uppercase"
        }
      ]
    }
  ]
}